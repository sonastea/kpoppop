{
  "name": "Kpoppop_Backend",
  "private": true,
  "version": "1.2.0",
  "description": "Kpoppop Server Backend",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development PORT=5000 nest start",
    "start:dev": "NODE_ENV=development PORT=5000 nest start --watch",
    "start:debug": "NODE_ENV=development PORT=5000 nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma generate && prisma migrate deploy && npm run build && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "start:docker": "npm run prisma:generate && npm run prisma:migrate && npm run start:prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "watch-prisma": "nodemon --watch prisma/schema.prisma --exec prisma generate"
  },
  "dependencies": {
    "@discord-nestjs/core": "^5.3.8",
    "@nestjs-modules/mailer": "^1.9.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/common": "^10.1.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.0",
    "@nestjs/platform-socket.io": "^10.1.0",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/websockets": "^10.1.0",
    "@oauth-everything/passport-discord": "^1.0.2",
    "@prisma/client": "^5.0.0",
    "@quixo3/prisma-session-store": "^3.1.13",
    "@socket.io/redis-adapter": "^8.2.1",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "discord.js": "^14.11.0",
    "ejs": "^3.1.9",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.10.1",
    "formidable": "^3.5.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "nodemailer": "^6.9.4",
    "passport": "0.5.3",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg-listen": "^1.7.0",
    "prisma-redis-middleware": "^4.8.0",
    "redis": "^4.6.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.2",
    "@types/nodemailer": "^6.4.8",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
