{
  "name": "kpoppop_backend",
  "private": true,
  "version": "1.4.3",
  "description": "Kpoppop Server Backend",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=development PORT=5000 nest start",
    "start:dev": "NODE_ENV=development PORT=5000 nest start --watch",
    "start:debug": "NODE_ENV=development PORT=5000 nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma generate && prisma migrate deploy && npm run build && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "start:docker": "npm run prisma:generate && npm run prisma:migrate && npm run start:dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "watch-prisma": "nodemon --watch prisma/schema.prisma --exec prisma generate"
  },
  "dependencies": {
    "@discord-nestjs/core": "^5.5.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.2",
    "@prisma/client": "^6.9.0",
    "@quixo3/prisma-session-store": "github:kleydon/prisma-session-store#5688463",
    "@types/compression": "^1.8.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "discord.js": "^14.19.3",
    "express-session": "^1.18.1",
    "firebase-admin": "^13.4.0",
    "formidable": "^3.5.4",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "pg-listen": "^1.7.0",
    "redis": "^5.5.5",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/passport": "^11.0.5",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.29",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "passport": "^0.5.3",
    "passport-discord-auth": "^1.2.0",
    "passport-local": "^1.0.0",
    "prisma": "^6.9.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "overrides": {
    "passport": "^0.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
