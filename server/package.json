{
  "name": "Kpoppop_Backend",
  "private": true,
  "version": "1.1.0",
  "description": "Kpoppop Server Backend",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:prod": "prisma generate && prisma migrate deploy && npm run build && npm run start:prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "watch-prisma": "nodemon --watch prisma/schema.prisma --exec prisma generate"
  },
  "dependencies": {
    "@discord-nestjs/core": "^5.2.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/common": "^9.3.7",
    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^9.3.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.2",
    "@nestjs/platform-express": "^9.3.7",
    "@nestjs/platform-socket.io": "^9.3.7",
    "@nestjs/throttler": "^4.0.0",
    "@nestjs/websockets": "^9.3.7",
    "@oauth-everything/passport-discord": "^1.0.2",
    "@prisma/client": "^4.10.0",
    "@quixo3/prisma-session-store": "^3.1.10",
    "@socket.io/redis-adapter": "^8.1.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.5.0",
    "formidable": "^2.1.1",
    "ioredis": "^5.3.0",
    "joi": "^17.7.0",
    "nodemailer": "^6.9.0",
    "passport": "0.5.3",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg-listen": "^1.7.0",
    "redis": "^4.6.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.1",
    "rxjs": "^7.8.0",
    "socket.io": "^4.5.4"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.7",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.13.0",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "prisma": "^4.10.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
